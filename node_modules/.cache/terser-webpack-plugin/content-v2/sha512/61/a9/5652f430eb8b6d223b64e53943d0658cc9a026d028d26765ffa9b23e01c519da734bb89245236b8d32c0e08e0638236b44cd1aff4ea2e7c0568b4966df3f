-router-dom\";\r\nimport ScrollAnimation from \"react-animate-on-scroll\";\r\nimport axios from \"../../axios\";\r\n\r\nconst NotificationCards = ({ textAlign, serviceStyle }) => {\r\n  const [datas, setDatas] = useState([]);\r\n  const [error, setError] = useState(null);\r\n  const getData = () => {\r\n    axios\r\n      .get(\"/notifications?limit=10\")\r\n      .then((resutl) => {\r\n        setDatas(resutl.data.data);\r\n        setError(null);\r\n      })\r\n      .catch((err) => {\r\n        let message = err.message;\r\n        setError(message);\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    getData();\r\n  }, []);\r\n  return (\r\n    <div className=\"row row--15 service-wrapper\">\r\n      {datas.map((val, i) => (\r\n        <div className=\"col-lg-4 col-md-6 col-sm-6 col-12\" key={i}>\r\n          <ScrollAnimation\r\n            animateIn=\"fadeInUp\"\r\n            animateOut=\"fadeInOut\"\r\n            animateOnce={true}\r\n          >\r\n            {error && <h1 color=\"danger\">{error}</h1>}\r\n            <div className={`service ${serviceStyle} ${textAlign}`}>\r\n              <div className=\"inner\">\r\n                <div className=\"image\">\r\n                  <img\r\n                    src=\"./images/fixing/magazine-black.png\"\r\n                    alt=\"card Images\"\r\n                  />\r\n                </div>\r\n                <div className=\"content\">\r\n                  <h4 className=\"title\">\r\n                    <Link\r\n                      to={`/notifications/${val._id}`}\r\n                      dangerouslySetInnerHTML={{ __html: val.name }}\r\n                    ></Link>\r\n                  </h4>\r\n                  <p\r\n                    className=\"description b1 color-gray mb--0\"\r\n                    dangerouslySetInnerHTML={{ __html: val.createdAt }}\r\n                  ></p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </ScrollAnimation>\r\n        </